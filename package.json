{
  "name": "emjay-car-wash-api",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/api/index.js",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc -w\"  \"nodemon dist/api/index.js\"",
    "lint": "eslint .",
    "test": "jest --coverage",
    "prepare": "husky"
  },
  "license": "MIT",
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "app.ts": [
      "prettier --write",
      "eslint --fix"
    ],
    "server.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "_moduleAliases": {
    "@routes": "dist/src/routes",
    "src": "dist/src"
  },
  "dependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.1",
    "bcrypt": "^5.1.1",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.6",
    "express": "^4.21.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongodb": "^6.11.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript-eslint": "^8.17.0"
  }
}
