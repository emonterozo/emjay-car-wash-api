{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@routes/*": ["src/routes/*"],
      "src/*": ["src/*"]
    },
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
}